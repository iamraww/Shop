<body>
<div class="container-fluid padding0">
    <%- include('partials/header-admin'); %>
    <div id="main">
        <%- include('partials/navbar'); %>
        <h1><span class="glyphicon glyphicon-globe addtop" aria-hidden="true"></span>&nbsp;&nbsp;Overview</h1>
        <div id="sub-main">
